Type: Package
Package: icecream
Title: Print Debugging Made Sweeter
Version: 0.1.1
Authors@R: 
    c(person(given = "Lewin",
             family = "Appleton-Fox",
             role = c("aut", "cre"),
             email = "lewin.a.f@gmail.com"),
      person(given = "Dominik",
             family = "Rafacz",
